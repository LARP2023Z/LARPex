<?xml version="1.0" encoding="UTF-8"?>   
<databaseChangeLog
   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:pro="http://www.liquibase.org/xml/ns/pro"
   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
      http://www.liquibase.org/xml/ns/pro 
      http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">
   <changeSet author="stankowd" id="1">
        <addForeignKeyConstraint baseTableSchemaName="games" baseTableName="player" baseColumnNames="user_id" constraintName="player_foreign_key_constraint"
            referencedTableSchemaName="users" referencedTableName="user" referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableSchemaName="events" baseTableName="user_event_relation" baseColumnNames="user_id" constraintName="uer_user_foreign_key_constraint"
            referencedTableSchemaName="users" referencedTableName="user" referencedColumnNames="id"/>


        <addForeignKeyConstraint baseTableSchemaName="events" baseTableName="user_event_relation" baseColumnNames="payment_id" constraintName="uer_payment_foreign_key_constraint"
            referencedTableSchemaName="payments" referencedTableName="payment" referencedColumnNames="id"/>


        <addForeignKeyConstraint baseTableSchemaName="events" baseTableName="user_event_relation" baseColumnNames="event_id" constraintName="uer_event_foreign_key_constraint"
            referencedTableSchemaName="events" referencedTableName="event" referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableSchemaName="games" baseTableName="play" baseColumnNames="event_id" constraintName="play_event_foreign_key_constraint"
            referencedTableSchemaName="events" referencedTableName="event" referencedColumnNames="id"/>


        <addForeignKeyConstraint baseTableSchemaName="games" baseTableName="play" baseColumnNames="game_id" constraintName="play_game_foreign_key_constraint"
            referencedTableSchemaName="games" referencedTableName="game" referencedColumnNames="id"/>


        <addForeignKeyConstraint baseTableSchemaName="games" baseTableName="player" baseColumnNames="play_id" constraintName="player_play_foreign_key_constraint"
            referencedTableSchemaName="games" referencedTableName="play" referencedColumnNames="id"/>
   </changeSet>
</databaseChangeLog>