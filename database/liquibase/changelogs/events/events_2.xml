<?xml version="1.0" encoding="UTF-8"?>   
<databaseChangeLog
   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:pro="http://www.liquibase.org/xml/ns/pro"
   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
      http://www.liquibase.org/xml/ns/pro 
      http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">
   <changeSet author="stankowd" id="2">
      <dropTable tableName="event" schemaName="events" />
      <createTable tableName="event" schemaName="events">
        <column name="id" autoIncrement="true" type="INTEGER">
            <constraints nullable="false"/>
        </column>
        <column name="status" type="VARCHAR(255)">
            <constraints nullable="false"/>
        </column>
      </createTable>
      <addPrimaryKey schemaName="events" tableName="event" columnNames="id" constraintName="event_primary_key_constraint"/>
   </changeSet>
</databaseChangeLog>