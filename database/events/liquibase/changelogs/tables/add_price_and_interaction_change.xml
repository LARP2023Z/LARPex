<?xml version="1.0" encoding="UTF-8"?>   
<databaseChangeLog
   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:pro="http://www.liquibase.org/xml/ns/pro"
   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
      http://www.liquibase.org/xml/ns/pro 
      http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">
   <changeSet author="stankowd" id="3">
      <createTable tableName="price" schemaName="dbo">
        <column name="id" type="UUID">
            <constraints nullable="false"/>
        </column>
        <column name="amount" type="decimal"/>
        <column name="type" type="varchar(255)"/>
        <column name="currency" type="varchar(3)"/>
        <column name="event_id" type="UUID"/>
      </createTable>
      <addPrimaryKey schemaName="dbo" tableName="price" columnNames="id" constraintName="price_primary_key_constraint"/>
      <addColumn schemaName="dbo" tableName="interaction">
        <column name="code" type="varchar(255)"/>
      </addColumn>
   </changeSet>
</databaseChangeLog>